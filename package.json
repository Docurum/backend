{
  "name": "api_starter",
  "version": "1.0.0",
  "description": "API Starter Created by @pinakipb2",
  "main": "server.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "nodemon --files src/server.ts",
    "build": "npx tsc && copyfiles -u 1 src/public/* build",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "test": "set DB_URL=${TEST_DB_URL} && npm run prisma:migrate && jest --watchAll",
    "prisma:migrate": "npx prisma migrate dev"
  },
  "keywords": [],
  "author": "Pinaki Bhattacharjee <pinakipb2@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0",
    "xss-clean": "^0.1.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.13",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
